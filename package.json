{
  "name": "vscode-sonamu-monorepo",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:ui": "pnpm -F naite-trace-viewer dev",
    "build": "pnpm -F naite-trace-viewer build && pnpm -F sonamu build",
    "build:extension": "pnpm -F sonamu build",
    "build:ui": "pnpm -F naite-trace-viewer build",
    "test": "pnpm -F sonamu test",
    "package": "pnpm -F sonamu package",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.6.0"
  }
}
